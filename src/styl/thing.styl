.lines path
    stroke-width: 3px
    fill: none

    &.parliament_mean
        stroke: $qz-purp-3

    &.ep_mean
        stroke: $qz-blue-3

.zero-line
    stroke: $qz-gray-3
    stroke-width: 3px
    fill: none

.left, .right
    font-family: $qz-sans-light
    stroke: $qz-gray-3
    fill: $qz-gray-3
    opacity: 0.8
